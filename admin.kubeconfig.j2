apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: {{ admin_kubeconfig_ca_cert | b64encode }}
    server: "{{ admin_kubeconfig_server }}"
  name: main
contexts:
- context:
    cluster: main
    user: system:kube-controller-manager
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:kube-controller-manager
  user:
    client-certificate-data: {{ admin_kubeconfig_client_cert | b64encode }}
    client-key-data: {{ admin_kubeconfig_client_key | b64encode }}
